{
	"name": "react-native-expo",
	"version": "1.0.0",
	"private": true,
	"main": "expo-router/entry",
	"scripts": {
		"start": "expo start",
		"android": "expo start --android",
		"ios": "expo start --ios",
		"web": "expo start --web",
		"web:dev": "expo start --web",
		"web:build": "expo export --platform web",
		"build": "echo 'Expo projects use eas build for production builds. Use expo export for web builds.'",
		"export": "expo export",
		"typecheck": "tsc --noEmit --pretty"
	},
	"dependencies": {
		"expo": "^53",
		"expo-status-bar": "^2",
		"react": "^19",
		"react-dom": "^19",
		"react-native": "^0.79",
		"react-native-web": "^0.20",
		"expo-router": "^5",
		"expo-linking": "^7",
		"expo-constants": "^17",
		"expo-splash-screen": "^0.30",
		"expo-system-ui": "^5",
		"react-native-safe-area-context": "^5.4",
		"react-native-screens": "~4.11"
	},
	"devDependencies": {
		"@babel/core": "^7",
		"@types/react": "^19",
		"@types/react-native": "latest",
		"typescript": "^5"
	}
}
